<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    {{>links}}
</head>
<body>
    <div class="container bg-primary">
        <h2>Data</h2>
    </div>
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
            </tr>
            </thead>
            <tbody>
                {{#each data}}
                <tr>
                    <td scope="row">{{ @index}}</td>
                    <td>{{this.name}}</td>
                    <td>{{this.email}}</td>
                    <td>{{this.phone}}</td>
                </tr>
            {{/each}}
            </tbody>
        
    </table>
    </div>
</body>
</html>